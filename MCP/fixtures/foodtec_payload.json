{
  "menu_export": {
    "request": {
      "jsonrpc": "2.0",
      "id": "menu-001",
      "method": "foodtec.export_menu",
      "params": {
        "orderType": "Delivery"
      }
    },
    "response": {
      "jsonrpc": "2.0", 
      "id": "menu-001",
      "result": {
        "categories": ["Appetizer", "Pizza", "Salad"],
        "total_items": 38
      }
    }
  },
  "order_validation": {
    "request": {
      "jsonrpc": "2.0",
      "id": "validate-001", 
      "method": "foodtec.validate_order",
      "params": {
        "draft": {
          "category": "Appetizer",
          "item": "3pcs Chicken Strips w/ FF",
          "size": "Lg",
          "price": 6.99,
          "customer": {
            "name": "Test User",
            "phone": "410-555-1234"
          }
        }
      }
    },
    "response": {
      "jsonrpc": "2.0",
      "id": "validate-001",
      "result": {
        "valid": true,
        "canonical_price": 7.41
      }
    }
  },
  "order_acceptance": {
    "request": {
      "jsonrpc": "2.0",
      "id": "accept-001",
      "method": "foodtec.accept_order", 
      "params": {
        "draft": {
          "category": "Appetizer",
          "item": "3pcs Chicken Strips w/ FF", 
          "size": "Lg",
          "price": 7.41,
          "customer": {
            "name": "Test User",
            "phone": "410-555-1234"
          }
        },
        "idem": "order-test-123"
      }
    },
    "response": {
      "jsonrpc": "2.0",
      "id": "accept-001", 
      "result": {
        "order_id": "32",
        "status": "accepted"
      }
    }
  }
}